{
  "name": "rello-backend",
  "version": "0.0.1",
  "description": "rello backend",
  "main": "./dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "eslint": "eslint ./src",
    "build:js": "babel src --out-dir dist --extensions \".ts\"",
    "build": "npm run eslint && npm run type-check && npm run build:js",
    "jest": "jest --runInBand --silent --bail --verbose --expand --passWithNoTests --collect-coverage test",
    "test": "npm run eslint && npm run type-check && npm run jest",
    "start": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
    "pm2": "pm2-runtime start pm2.config.cjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-source-map-support": "^2.2.0",
    "eslint": "^8.47.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "7.23.8",
    "@types/express": "^4.17.21",
    "awilix": "^10.0.1",
    "express": "^4.18.2",
    "jest": "29.7.0",
    "pm2": "^5.3.0"
  }
}
